<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <%= f.label :artist_id %>
  <%= f.text_field :artist_name %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.fields_for :notes do |note|%>
    <%= note.label :content %>
    <%= note.text_field :content %>
  <% end %>

  <%= f.submit %>
<% end %>


